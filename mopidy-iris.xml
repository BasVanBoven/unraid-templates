<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Mopidy</Name>
  <Description></Description>
  <Registry>https://hub.docker.com/r/jaedb/iris</Registry>
  <GitHub>https://github.com/jaedb/Iris</GitHub>
  <Repository>jaedb/Iris</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>6600</HostPort>
        <ContainerPort>6600</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>6680</HostPort>
        <ContainerPort>6680</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>6681</HostPort>
        <ContainerPort>6681</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/mopidy/mopidy.conf</HostDir>
      <ContainerDir>/mopidy.conf</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/mopidy/tmp/</HostDir>
      <ContainerDir>/tmp/snapfifo</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/syncthing/Data/Audio/library/</HostDir>
      <ContainerDir>/media</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:6680]/iris/</WebUI>
  <Version></Version>
  <Icon>https://raw.githubusercontent.com/BasVanBoven/unraid-templates/master/icons/mopidy.png</Icon>
  <Beta>False</Beta>
  <Category>Network:Other</Category>
  <Date>2021-04-17</Date>
</Container>
